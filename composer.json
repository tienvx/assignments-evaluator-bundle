{
    "name": "tienvx/assignments-evaluator-bundle",
    "description": "Symfony Bundle for Assignments Evaluator",
    "type": "symfony-bundle",
    "require": {
        "php": "^7.4|^8.0",
        "tienvx/assignments-evaluator": "^1.0",
        "symfony/http-kernel": "^5.4|^6.0",
        "symfony/validator": "^5.4|^6.0",
        "symfony/dependency-injection": "^5.4|^6.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "doctrine/annotations": "^1.13"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Tienvx\\Bundle\\AssignmentsEvaluatorBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tienvx\\Bundle\\AssignmentsEvaluatorBundle\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Tien Vo Xuan",
            "email": "tien.xuan.vo@gmail.com",
            "homepage": "https://tienvx.github.io/"
        }
    ],
    "conflict": {
        "symfony/symfony": "*"
    }
}
